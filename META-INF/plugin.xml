<plugin>
  <author>Electric Cloud</author>
  <authorUrl>http://www.electric-cloud.com/support</authorUrl>
  <category>Container Management</category>
  <commander-version min="6.0" />
  <description>Integrates with Docker to deploy containers and services on a stand-alone Docker host or a Docker Swarm cluster.</description>
  <help>help.xml</help>
  <key>EC-Docker</key>
  <configure>ec_config</configure>
  <label>EC-Docker</label>
  <version>1.3.0</version>

  <customTypes>
    <customType name="Artifact2Image">
      <parameterPanel>
        <javascript>war/ecplugins.EC_Docker.Artifact2ImageParameterPanel/ecplugins.EC_Docker.Artifact2ImageParameterPanel.nocache.js</javascript>
        <help>/commander/pages/@PLUGIN_KEY@-@PLUGIN_VERSION@/EC-Docker_help#Artifact2Image</help>
      </parameterPanel>
    </customType>
  </customTypes>
</plugin>
